{
  "name": "location-server",
  "version": "0.0.1",
  "description": "server for simple location based chat app",
  "main": "index.js",
  "scripts": {
    "test": "ava",
    "dev": "nodemon --watch src/ --exec babel-node src/index.js"
  },
  "author": "khubo",
  "license": "MIT",
  "devDependencies": {
    "@babel/register": "^7.0.0-beta.39",
    "ava": "^1.0.0-beta.1",
    "axios": "^0.17.1",
    "babel-cli": "^7.0.0-beta.0",
    "babel-preset-env": "^2.0.0-beta.2",
    "nodemon": "^1.14.12"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "georedis": "^3.1.1",
    "jsonwebtoken": "^8.1.1",
    "redis": "^2.8.0",
    "winston": "^2.4.0"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "sources": [
      "src/**/*.js"
    ],
    "failFast": true,
    "failWithoutAssertion": false,
    "require": [
      "@babel/register"
